/opt/radiaTest/radiaTest-messenger/log/messenger/stderr.log {
    su root root
    missingok
    daily
    rotate 90
    maxsize 1M
    copytruncate
    notifempty
    nocompress
    dateext
    dateformat -%Y%m%d.%s
}

